{
  "name": "lubtuts",
  "version": "1.0.0",
  "description": "Lubtuts react version",
  "scripts": {
    "start": "APP_ID=libtuts NODE_ENV=development node index.js",
    "start:prod": "APP_ID=libtuts NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "minify": "cleancss -o static/dist/app.min.css static/dist/app.css",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "gzip": "npm run gzip:js && npm run gzip:css",
    "gzip:css": "gzip -c static/dist/app.min.css > static/dist/app.min.css.gz",
    "gzip:js": "gzip -c static/dist/bundle.min.js > static/dist/bundle.min.js.gz",
    "worker:seed": "DEBUG=app:* node workers/seed",
    "worker:indexer": "DEBUG=app:*,indexer node workers/indexer",
    "worker:queue:search": "DEBUG=app:*,searchWorker node workers/queues/search",
    "worker:queue:tutorials": "DEBUG=app:* node workers/queues/tutorials"
  },
  "dependencies": {
    "async": "^2.0.0-rc.1",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "body-parser": "^1.15.0",
    "config": "^1.19.0",
    "debug": "^2.2.0",
    "elasticsearch": "^10.1.3",
    "express": "^4.13.4",
    "history": "^2.0.1",
    "lodash": "^4.6.1",
    "marked": "^0.3.5",
    "react": "^15.0.0-rc.2",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-dom": "^15.0.0-rc.2",
    "react-mixin": "^3.0.4",
    "react-router": "^2.0.1",
    "react-select": "^1.0.0-beta11",
    "react-timeago": "^2.2.1",
    "reactfire": "^0.6.0",
    "request": "^2.69.0",
    "stylus": "^0.54.2"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.7.2",
    "clean-css": "^3.4.10",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
