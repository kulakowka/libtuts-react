{
  "name": "lubtuts",
  "version": "1.0.0",
  "description": "Lubtuts react version",
  "scripts": {
    "start": "cross-env NODE_ENV=development node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "minify": "cleancss -o static/dist/app.min.css static/dist/app.css",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install"
  },
  "dependencies": {
    "babel-core": "^6.6.5",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "firebase": "^2.4.1",
    "history": "^2.0.1",
    "react": "^0.14.7",
    "react-addons-linked-state-mixin": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-mixin": "^3.0.3",
    "react-router": "^2.0.0",
    "reactfire": "^0.6.0",
    "stylus": "^0.54.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.6.5",
    "clean-css": "^3.4.10",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
  }
}
