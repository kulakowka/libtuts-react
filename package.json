{
  "name": "lubtuts",
  "version": "1.0.0",
  "description": "Lubtuts react version",
  "scripts": {
    "start": "cross-env NODE_ENV=development node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "minify": "cleancss -o static/dist/app.min.css static/dist/app.css",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install"
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "firebase": "^2.4.1",
    "history": "^1.17.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0-rc5"
  },
  "devDependencies": {
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-register": "^6.4.3",
    "clean-css": "^3.4.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
